			$(".con-0").on("click", function() {
				var umsd = $(this).parent().parent().find(".mwsae-box").length;
				ads++;
				if(ads >= umsd - 1) {
					ads = -1;
				}
				$(this).parent().parent().find(".mwsae-box").eq(ads).fadeIn(300).siblings(".mwsae-box").hide();
			})
			$(".con-1").on("click", function() {
				var umsd = $(this).parent().parent().find(".mwsae-box").length;
				umk++;
				if(umk >= umsd - 1) {
					umk = -1;
				}
				$(this).parent().parent().find(".mwsae-box").eq(umk).fadeIn(300).siblings(".mwsae-box").hide();
			})

			$(".cen-tnm").on("click", function() {
				var wm = $(this).find("a").attr("date-name")
				jm = $(this).find("a").attr("date-tex")
				im = $(this).find("a").attr("date-img")
				tit = $(this).find("a").text();
				$(this).parent().parent().find("img").attr("src", im);
				$(this).parent().parent().find(".mi-tex").html("<span>" + jm + "</span> by <span>" + wm + "</span>");
				$(this).parent().parent().find(".imd-busd").html(tit);
			})
			$("div.holder").jPages({
				containerID: "itemContainer", //需要被分页的容器
				previous: "", //前进后退 内容字
				next: "",
				first: "首页",
				last: "尾页",
				startPage: 1, //起始显示的页码
				perPage: 4, //每页的内容数量
				midRange: 2, //分页器中间页数
				startRange: 1, //开头页数
				endRange: 1 //结尾页数	
			});
		</script>
	</body>

</html>