						var IE5 = IE55 = IE6 = IE7 = IE8 = false;
						var reIE = new RegExp("MSIE (\\d+\\.\\d+);");
						reIE.test(userAgent);
						var fIEVersion = parseFloat(RegExp["$1"]);
						IE55 = fIEVersion == 5.5;
						IE6 = fIEVersion == 6.0;
						IE7 = fIEVersion == 7.0;
						IE8 = fIEVersion == 8.0;
						if(IE55) {
							return "IE55";
						}
						if(IE6) {
							return "IE6";
						}
						if(IE7) {
							return "IE7";
						}
						if(IE8) {
							return "IE8";
						}
					} //isIE end
