body,ul,ol,dl{
	margin: 0;
	padding: 0;
	list-style-type: none;
	font-size: 16px;
	font-family: "微软雅黑";
}
body{
overflow-x:hidden ;	
}
a{
	text-decoration: none!important;
	border: 0px;
}
.fh5co-cards{
	background: #fff;
}
.clear{
	clear:both;
}
.img-wdith{
width:100%;	
height:600px;
background: url(../images/bing.png) center no-repeat;	
background-size:100% 100%;
line-height: 600px;
text-align: center;
font-size: 3em;
color:#fff;
font-weight: 700;
text-shadow: 0.1em 0.1em 0.2em rgba(0,0,0,0.5);
overflow-x:hidden ;
/**/
}
.black-box{
width:50%;	
height:60px;	
line-height: 60px;
background: rgba(0,0,0,0.6);
margin-left: -25%;
position: absolute;
left:50%;
bottom:0px;
filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#7f000000,endColorstr=#7f000000);
z-index: 9999;
}
.black-box span:first-child{
	font-size:2em;
	color:#fff;
	margin-left: 1em;
	margin-right: 0.5em;
}
.black-box span:last-child{
position:relative;	
top:-4px;
margin-left:0.4em;	
font-size: 0.9em;
}
.black-box span:last-child a{
	color:#fff;
}
#fh5co-hero{
/*height:300px!important;*/	
background-size:cover ;
    filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(
    src='../images/bing.png',
    sizingMethod='scale');
}
#fh5co-hero .overlay{
background: none!important;
position: relative;		
}

#fh5co-hero .fh5co-hero-wrap .fh5co-hero-intro > h1{
font-size: 5em;
color:#fff;
text-align: center;
font-weight: 500;
text-shadow: 0.1em 0.1em 0.2em rgba(0,0,0,0.5)!important;		
}
.container-fluid h2{
font-size: 1.8em;	
}
.pos-left{	
width:30%;	
background: url(../images/hen.png) no-repeat right;
height:50px;
margin-top: 8px;
}
.cen-text{
width:16%;
font-size: 2em;
color:#000000;
}
.pos-right{
	width:30%;
background: url(../images/hen-2.png) no-repeat left;
height:50px;
