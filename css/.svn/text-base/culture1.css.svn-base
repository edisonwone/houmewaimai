#fh5co-hero {
	/*height:300px!important;*/
	background-size: cover;
	filter: progid:DXImageTransform.Microsoft.AlphaImageLoader( src='http://127.0.0.1:8020/xiangguguanwang/images/bing.png', sizingMethod='scale');
}

@media screen and (min-width: 480px) {
	.col-md-4,
	.col-xs-4,
	.col-sm-4 {
		padding-left: 7px;
		padding-right: 7px;
		padding-bottom: 1.5em;
	}
	.col-md-12,
	.col-xs-12,
	.col-sm-12 {
		padding-bottom: 1.5em;
	}
	.col-xxs-12 {
		padding-left: 7px;
		padding-right: 7px;
	}
}

@media screen and (max-width: 480px) {
	.col-xxs-12 {
		padding-left: 7px;
		padding-right: 7px;
	}
	.black-box span:first-child {
		font-size: 1em;
	}
	.col-md-12,
	.col-xs-12,
	.col-sm-12 {
		padding-bottom: 1.5em;
	}
	.col-md-4,
	.col-xs-4,
	.col-sm-4 {
		padding-bottom: 1.5em;
	}
}

#fh5co-hero .fh5co-hero-wrap .fh5co-hero-intro>h1 {
	font-family: "微软雅黑";
	line-height: 47px;
	color: #ffffff;
	font-weight: 100;
	text-shadow: 3px 3px 8px rgba(0, 0, 0, 0.8);
}

#fh5co-features {
	padding: 4em 0;
}

.heading {
	margin-bottom: 4em;
}

@media screen and (max-width: 992px) {
	.circle-bg {
		width: 100%;
	}
}

@media screen and (max-width: 768px) {
	.contant {
		width: 100%;
		height: auto;
		margin: 0 auto;
	}
	.zhanlveback {
		width: 100%;
		margin: 0 auto;
		height: auto;
	}
	.zhanlvepic {
		width: 80%;
		margin-top: 0px;
	}
	.contant .back-box {
		width: 85%;
	}
	.zhanlveback3 {
		width: 100%;
		margin: 0 auto;
	}
	.circle-box {
		width: 100%;
		margin-top: 30px;
	}
	.text {
		/*top: -105px;*/
		position: relative;
		font-size: 26px;
	}
	.guihuabox {
		width: 100%;
	}
	#fh5co-main .row .contant .back-box p {
		font-size: 16px;
	}
	#fh5co-features .fh5co-feature-icon {
		/*height: 80px!important;*/
	}
}

.contant .back-box {
	background: url(../img/background.png) no-repeat center;
	background-size: 100%;
	margin: 0 auto;
}

.contant .back-box2 {
	background: url(../img/back2.png) no-repeat;
	background-size: 100%;
	margin: 0 auto;
}

.contant .back-box {
	width: 60%;
}

@media screen and (max-width: 768px) {
	.contant .back-box {
		width: 100%;
	}
	
	.marginb{
		margin-bottom: 20px;
	}
}

@media screen and (min-width: 769px) {
	.contant {
		width: 100%;
	}
	.zhanlveback {
		width: 100%;
		margin: 0 auto;
	}
	.zhanlvepic {
		width: 80%;
		margin-top: 50px;
		margin-bottom: 50px;
	}
	.contant .back-box {
		width: 60%;
	}
	.zhanlveback2 {
		width: 50%;
		margin: 0 auto;
	}
	.zhanlveback3 {
		width: 60%;
		margin: 0 auto;
	}
	.circle-bg .circle-box {
		width: 20%;
	}
	.circle-bg {
		width: 80%;
	}
	.text {
		/*top: -115px;*/
		position: relative;
		font-size: 26px;
	}
	.guihuabox {
		width: 500px;
	}
	#fh5co-main .row .contant .back-box p {
		font-size: 20px;
		color: #5f3100;
	}
}

#fh5co-main .row .contant .back-box2 p {
	font-size: 18px;
	color: #5f3100;
	margin-bottom: 10px;
}

.col-sm-12 {
	padding: 0;
}

#fh5co-features .fh5co-feature-icon {
	/*height: 80px;*/
}

.guihuabox ul li {
	margin-bottom: 60px;
}

.title {
	text-align: left;
	line-height: 40px;
	color: #5f3100;
	font-size: 25px;
	text-indent: 2%;
}

.guihua-left-box {
	height: 200px;
	width: 150px;
	margin: 0 auto;
	border: 1px #333333 solid;
}

.guihua-left-text {
	font-size: 40px;
	color: #333333;
	background: #f9d3d4;
}

.guihua-left-smtext {
	height: 17px;
	width: 100px;
	background: #f9d3d4 url(../img/pinktext.png)no-repeat 0px 3px;
	background-size: 100%;
	margin-left: 10px;
}

.guihuabox {
	height: auto;
	margin: 0 auto;
	border-left: 2px #777777 solid;
}

.guihuabox ul {
	padding-top: 50px;
}

.guihuabox ul li .point {
	width: 10px;
	height: 10px;
	background: url(../img/circle.png) no-repeat;
	background-size: 100%;
	position: relative;
	left: -6px;
}

.guihuabox ul li .describe {
	width: 90%;
	height: auto;
	background: url(../img/backgray.png) no-repeat;
	background-size: 100% 100%;
	border-radius: 5px;
	margin-left: 20px;
	margin-top: -20px;
}

.guihuabox ul li .describe-time {
	border-bottom: 1px #FFFFFF solid;
	margin: 0 10px 0 20px;
	color: #968780!important;
	padding: 5px 0;
}

.guihuabox ul li .describe-detail {
	margin: 0 10px;
	color: #968780!important;
	padding: 5px
}

#fh5co-features .fh5co-feature-icon i {
	color: #DDDDDD;
}

html.fix-ie-font-face :before,
html.fix-ie-font-face :after {
	content: none !important;
}

.jianli {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background: rgba(255, 208, 46, 0.9);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#9fffd02e, endColorstr=#9fffd02e);
}

.jianli h4 {
	width: 70%;
	margin: 5% auto;
	border: 3px #333 solid;
	padding: 3%;
	text-align: center;
	font-size: 22px;
	color: #333;
}

.jianli p {
	text-align: center;
	color: #333;
}

#fh5co-features p {
	margin: 0;
	padding: 0;
}

.col-xs-12 {
	padding: 0;
}

.padding {
	padding: 20px;
}

@media only screen and (min-width: 100px) and (max-width: 490px) {
	
	.jionus-xx{
		width: 100%;
	}
	.padding {
		padding: 0;
	}
	.padding p{
		margin: 0;
		padding: 2%;
	}
	.jianli h4 {
		margin: 0 auto
	}
	.jianli p{
		margin: 0;
	}
}